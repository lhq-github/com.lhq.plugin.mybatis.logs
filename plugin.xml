<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="com.lhq.plugin.mybatis.logs.commands.sampleCommand"
            class="com.lhq.plugin.mybatis.logs.handlers.PluginHandler">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="com.lhq.plugin.mybatis.logs.toolbars.sampleToolbar">
            <command
                  commandId="com.lhq.plugin.mybatis.logs.commands.sampleCommand"
                  icon="icons/sample.jpg"
                  tooltip="mybatis plugin"
                  id="com.lhq.plugin.mybatis.logs.toolbars.sampleCommand">
            </command>
         </toolbar>
      </menuContribution>
   </extension>

</plugin>
